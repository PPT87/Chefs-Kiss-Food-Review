<% if (!user) { %>
  <link rel="stylesheet" href="/stylesheets/siwg.css">
<% } %>
</head>
<body>
  <div class="nav-wrapper">  
  <nav class="navbar">
    <a href="">nav-logo</a>
  <ul class="nav-menu">
    <li class="nav-items"><a href="/foods" class="nav-link">My Reviews</a></li>
    <li class="nav-items"><a href="/foods/new" class="nav-link">Add New Review</a></li>
    <li class="nav-items"><a href="/profiles" class="nav-link">Community Page</a></li>
    <li class="nav-items"><% if (user) { %>
      <a href="/auth/logout" class="nav-link">Logout</a>
      <% } else { %>
        <a href="/auth/google" class="siwg-link">
          <button type="submit" class="siwg">
            <img id="siwg-image" alt="Sign in with Google" />
          </button>
        </a>
      <% } %>
    </li>
  </ul>
  <div class="hamburger">
    <span class="bar"></span>
    <span class="bar"></span>
    <span class="bar"></span>     
  </div>
</nav>
</div>


